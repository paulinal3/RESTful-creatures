<form action="/dinosaurs" method="GET">
    <label for="nameFilter">Filter By Name</label>
    <input id="nameFilter" type="text" name="nameFilter">
    <input type="submit">
</form>

<ul>
    <% dinoData.forEach((dino, index) => { %>
        <li>
            <h4><%= dino.name %> is a <%= dino.type %></h4>
            <form action="/dinosaurs/<%= index %>/?_method=DELETE" method="POST">
                <input type="submit" value="Delete">
            </form>
        </li>
    <% }) %> 
</ul>